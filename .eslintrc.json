{
    "extends": "airbnb",
    "rules": {
        "no-undef": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-console": 0,
        "no-plusplus": 0,
        "indent": 0
    }
}
