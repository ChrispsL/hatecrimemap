{
    "extends": "airbnb",
    "rules": {
        "no-undef": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-console": 0,
        "no-plusplus": 0,
        "indent": 0,
        "no-alert": 0,
        "jsx-a11y/label-has-for": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}
